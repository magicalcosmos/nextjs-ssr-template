    "pm2:docker:start": "pm2-runtime start ecosystem.config.js",
    "pm2:start": "pm2 restart ecosystem.config.js",
    "pm2:docker:start-test": "pm2-runtime start ecosystem.test.config.js",
    "pm2:start-test": "pm2 start ecosystem.test.config.js",
